{
    "name": "webgriffe/esb",
    "description": "Simple, beanstalkd powered, ESB framework.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Webgriffe SRL",
            "email": "support@webgriffe.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "symfony/dependency-injection": "^3.3",
        "symfony/config": "^3.3",
        "symfony/yaml": "^3.3",
        "amphp/beanstalk": "^0.2.4",
        "monolog/monolog": "^1.23",
        "dragonmantank/cron-expression": "^2.0",
        "twig/twig": "^2.5",
        "nikic/fast-route": "^1.3",
        "amphp/http-server": "^0.8.2",
        "amphp/amp": "^2.1",
        "psr/log": "^1.1",
        "amphp/socket": "^0.10",
        "amphp/http": "^1.1",
        "amphp/file": "^0.3",
        "amphp/log": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "Webgriffe\\Esb\\": ["src/"]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Webgriffe\\Esb\\": ["tests/"]
        }
    },
    "bin": [
        "bin/esb"
    ],
    "require-dev": {
        "phpunit/phpunit": "^6.5",
        "amphp/phpunit-util": "^1.0",
        "pda/pheanstalk": "^3.1",
        "mikey179/vfsstream": "^1.6",
        "amphp/artax": "^3.0",
        "phpstan/phpstan": "^0.9.2",
        "squizlabs/php_codesniffer": "^3.2"
    }
}
